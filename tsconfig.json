{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "strict": true,
    "lib": ["DOM", "esnext"],
    "declaration": true,
    "declarationMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@constant/*": ["./src/constant/*"],
      "@dataFormat/*": ["./src/dataFormat/*"],
      "@typeCreator/*": ["./src/typeCreator/*"],
      "@utils/*": ["./src/utils/*"]
    }
  },
  "include": ["./src"],
  "exclude": ["./node_modules"]
}
