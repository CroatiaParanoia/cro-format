{
  "name": "ffformat",
  "version": "1.1.2-4",
  "description": "",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "scripts": {
    "lint": "eslint src/ --ext .ts",
    "test": "jest --config=jest.config.js --watchAll",
    "clear": "rm -rf ./lib ./types",
    "build": "npm run clear && webpack",
    "release": "ts-node ./scripts/release.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --pattern='src/*' --staged && npm run lint && npm run test -- --watchAll=false && npm run build",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CroatiaParanoia/ffformat.git"
  },
  "keywords": [
    "data",
    "format"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CroatiaParanoia/ffformat/issues"
  },
  "homepage": "https://github.com/CroatiaParanoia/ffformat#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/types": "^7.9.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.1",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-jest": "^25.3.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.11.0",
    "eslint-config-alloy": "^3.8.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "husky": "^4.2.5",
    "inquirer": "^7.1.0",
    "jest": "^25.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "semver": "^7.3.2",
    "ts-loader": "^8.0.6",
    "ts-node": "^8.9.0",
    "typescript": "^4.0.3",
    "vinyl-buffer": "^1.0.1",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0"
